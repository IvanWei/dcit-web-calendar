{
  "name": "dcit-web-calendar",
  "version": "0.0.0",
  "description": "Developer Conferences In Taiwan",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "next dev -p 8080 --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "next lint",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:fix": "next lint --fix && stylelint \"**/*.css\" --fix",
    "postbuild": "next-sitemap --config ./next-sitemap.config.mjs && npm run rss",
    "rss": "node ./lib/generateRSS.mjs > ./out/rss.xml",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/IvanWei/dcit-web-calendar.git"
  },
  "keywords": [
    "dcit",
    "developer",
    "calendar",
    "conference",
    "taiwan",
    "行事曆"
  ],
  "author": "Wei Hong-Lin <scorpionseed@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IvanWei/dcit-web-calendar/issues"
  },
  "engines": {
    "node": ">=16.8.0"
  },
  "browserslist": [
    "chrome 64",
    "edge 79",
    "firefox 67",
    "opera 51",
    "safari 12"
  ],
  "homepage": "https://github.com/IvanWei/dcit-web-calendar#readme",
  "devDependencies": {
    "@types/node": "20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "stylelint": "^15.9.0",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "feed": "^4.2.2",
    "next": "^13.4.7",
    "next-sitemap": "^4.1.8",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.1",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.1",
    "sweetalert2": "^11.7.12",
    "sweetalert2-react-content": "^5.0.7",
    "swr": "^2.2.0"
  }
}
